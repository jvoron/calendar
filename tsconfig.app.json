{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/spec",
    "types": [
      "jasmine",
      "node"

    ]
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts",
    "test.ts",
    "polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts",
    
    "**/*.spec.ts",
    "**/*.d.ts"
  ]
}
